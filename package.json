{
  "license": "MIT",
  "name": "mobile_backend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "eslint-f": "eslint app --fix",
    "prettier-f": "prettier app --write -c",
    "ts-f": "tsc --project tsconfig.json",
    "fix-esm-import-path-app": "fix-esm-import-path app",
    "ts": "run-s ts-f fix-esm-import-path-app eslint-f prettier-f",
    "start": "node ./app/app.js",
    "dev": "DEBUG=knex:query nodemon --exec node ./app/app.js",
    "db:create_migration": "yarn knex migrate:make --knexfile ./knexFile.js",
    "db:create_seed": "yarn knex seed:make --knexfile ./knexFile.js",
    "db:migrate": "yarn knex migrate:latest --knexfile ./knexFile.js && yarn db:annotate",
    "db:rollback": "yarn knex migrate:down --knexfile ./knexFile.js && yarn db:annotate",
    "db:seed": "yarn knex seed:run --knexfile ./knexFile.js",
    "db:create": "npx run-func bin/db.rake.js create && yarn db:annotate",
    "db:drop": "npx run-func bin/db.rake.js drop",
    "db:annotate": "npx run-func bin/db.rake.js annotate",
    "db:setup:complete": "run-s db:drop db:create db:migrate db:seed",
    "db:setup:test": "NODE_ENV=test run-s db:drop db:create db:migrate",
    "db:setup:all": "run-s db:setup:complete db:setup:test",
    "knex": "node_modules/.bin/knex",
    "prod": "NODE_ENV=production yarn db:migrate && NODE_ENV=production node --experimental-loader=newrelic/esm-loader.mjs ./app/app.js",
    "test": "NODE_ENV=test mocha --recursive  --exit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@local/app": "link:./app",
    "@local/app.js": "link:./app/app.js",
    "@local/connectors": "link:./app/connectors",
    "@local/constants": "link:./app/constants",
    "@local/graphql": "link:./app/graphql",
    "@local/helpers": "link:./app/helpers",
    "@local/lib": "link:./app/lib",
    "@local/routes": "link:./app/routes",
    "@local/services": "link:./app/services",
    "@types/uuid": "^9.0.5",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-ws": "^5.0.2",
    "graphql": "^16.6.0",
    "graphql-date": "^1.0.3",
    "graphql-request": "^6.1.0",
    "graphql-type-datetime": "^0.2.4",
    "graphql-type-json": "^0.3.2",
    "i18next": "^23.7.7",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "newrelic": "^10.4.2",
    "node-cron": "^3.0.2",
    "pg": "^8.11.0",
    "react-i18next": "^13.5.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-svg": "^15.1.0",
    "react-use-websocket": "3.0.0",
    "redis": "^4.6.7",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2",
    "ws": "^8.13.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@local/test": "link:./test",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.8.4",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.41.0",
    "eslint-config-leapfrog": "^3.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "fix-esm-import-path": "^1.4.0",
    "foreman": "^3.0.1",
    "graphql-request": "^6.1.0",
    "husky": "^8.0.3",
    "knex-types": "^0.5.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "request": "^2.88.2",
    "run-func": "^3.0.0",
    "sinon": "^15.1.0",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "typescript": "^5.2.2"
  }
}
